    <script>

        function selectMenu(menuItem) {
            // xóa menu đã chọn trước đó.
            var menuItems = document.querySelectorAll('.menu-item.active');
            menuItems.forEach(function (item) {
                item.classList.remove('active');
            });

            // lấy menu hiện tại.
            var element = document.getElementById(menuItem);
            element.classList.add('active');
        }

    </script>

    <script>
        // get element password value:
        const textBox_Password = document.getElementById("MatKhau");

        // input password:
        textBox_Password.addEventListener("input", function () {
            // setup:
            const keyValues = this.value; // get value
            // get message
            const message_TextDanger = document.getElementById("ValidPasswordByJS");
            let message_warning = "";

            // get lables:
            const lableWarning = document.getElementById("passwordWaring");
            const lableDanger = document.getElementById("passwordDanger");
            const lableSuccess = document.getElementById("passwordSuccess");

            // Reset labels:
            resetElement(
                lableWarning,
                lableDanger,
                lableSuccess,
                message_warning,
                message_TextDanger
            );

            // Check password issues
            if (keyValues.toLowerCase() == 'gs755@@.local' || keyValues.toLowerCase() == 'gs755') {
                lableDanger.style.display = "inline-block";
                message_warning = "từ ngữ xúc phạm nhân quyền ! chim kút";
            }


            else if (keyValues.length < 3) {
                lableDanger.style.display = "inline-block";
                message_warning = "Password must be greater or equal 6 characters.";
            } else if (keyValues == '123456') {
                lableDanger.style.display = "inline-block";
                message_warning = "Password is very popular. Please! choose another password. (optional) Hê hê !";
            }
            else if (!/[0-9]/.test(keyValues) || !/[A-Z]/.test(keyValues) || !/[a-z]/.test(keyValues) || !/[!@@'#$%^&*(),.?":{}|<>]/.test(keyValues)) {

                lableDanger.style.display = "inline-block";
                lableDanger.classList.remove("bg-danger");
                lableDanger.classList.add("bg-warning");

                lableWarning.style.display = "inline-block";
                message_warning = "password is not a strong password! a strong password is a password with: 1 capital letter. 2. lowercase letters. 3. special characters.\n4. has numerals.";
            }
            else {
                strongPassword(
                    lableSuccess,
                    message_warning,
                    message_TextDanger
                );
                message_warning = "password is very 💪 haha.";
            }

            message_TextDanger.textContent = message_warning;
        });

        textBox_Password.addEventListener("blur", function () {
            const keyValues = this.value;

            // Nếu không có ký tự nào, ẩn tất cả các nhãn
            if (keyValues.length === 0) {
                document.getElementById("ValidPasswordByJS").textContent = "";
                document.getElementById("passwordWaring").style.display = "none";
                document.getElementById("passwordDanger").style.display = "none";
                document.getElementById("passwordSuccess").style.display = "none";
            }
        });

        function resetElement(lableWarning, lableDanger, lableSuccess, message_warning, message_TextDanger) {

            lableWarning.style.display = "none";
            lableDanger.style.display = "none";
            lableSuccess.style.display = "none";

            message_warning = "";
            message_TextDanger.classList.remove("text-success");
            message_TextDanger.classList.add("text-danger");

            lableDanger.classList.remove("bg-warning");
            lableDanger.classList.add("bg-danger");
        }

        function strongPassword(lableSuccess, message_warning, message_TextDanger) {

            lableSuccess.style.display = "block";
            message_TextDanger.classList.remove("text-danger");
            message_TextDanger.classList.add("text-success");
            message_TextDanger.textContent = message_warning;

        }

    </script>
